<h1>Klassifizierung</h1>
{#
    TOPIC CATEGORY  
#}
{% set id = 'p[topicCategory][]' %}

<div class="oneColBox">
    <label>
        <div class="icon mandatory"></div>{{'Themenkategorie'|trans}}:
    </label>
    <select class="oneCol1 no-value" id="select_{{ id|md_id }}">
        <option value=""></option>
        <option value="intelligenceMilitary">Aufklärung/Militär</option>
        <option value="structure">Bauwerke</option>
        <option value="imageryBaseMapsEarthCover">Bilddaten/Basiskarten/Landbedeckung</option>
        <option value="inlandWaters">Binnengewässer</option>
        <option value="biota">Biologie</option>
        <option value="geoscientificInformation">Geowissenschaften</option>
        <option value="society">Gesellschaft</option>
        <option value="health">Gesundheitswesen</option>
        <option value="boundaries">Grenzen</option>
        <option value="elevation">Höhenangaben</option>
        <option value="climatologyMeteorologyAtmosphere">Klimatologie/Meteorologie/Atmosphäre</option>
        <option value="farming">Landwirtschaft</option>
        <option value="oceans">Meere</option>
        <option value="location">Ortsangaben</option>
        <option value="planningCadastre">Planungsunterlagen/Kataster</option>
        <option value="environment">Umwelt</option>
        <option value="utilitiesCommunication">Ver- und Entsorgung/Nachrichtenwesen</option>
        <option value="transportation">Verkehrswesen</option>
        <option value="economy">Wirtschaft</option>
    </select>
    <div class="btn-group">
        <div class="last showHelp"  {{ id|md_data_obj }}>
            <div class="icon help"></div>
        </div>
    </div>

    <div class="clear"></div>

    <div class="singleResult" id="result_{{ id|md_id }}">
        {% if p['topicCategory'] is defined %}
        {% for value in p['topicCategory'] %}
        <div>
            <input type="hidden" name="{{ id }}" value="{{ value }}" />
            <label>{{ value }}</label>
            <div class="btn cmdDeleteSingleValue">
                <div class="icon delete"></div>
            </div>
        </div>
        {% endfor %}
        {% endif %}
        <div class="clear"></div>
    </div>
</div>

