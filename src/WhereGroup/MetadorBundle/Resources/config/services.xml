<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="metador_twig.class">WhereGroup\MetadorBundle\Twig\Extension\MetadorExtension</parameter>
        <parameter key="metador_metadata_import.class">WhereGroup\MetadorBundle\Component\MetadataImport</parameter>
    </parameters>

    <services>
        <service id="metador.twig_extension" class="%metador_twig.class%">
            <argument type="service" id="service_container" />
            <tag name="twig.extension" />
        </service>
        <service id="metadata_import" class="%metador_metadata_import.class%" />
    </services>
</container>